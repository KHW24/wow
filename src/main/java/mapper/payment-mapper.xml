<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Payment">
	<resultMap id="resultPayment" type="Payment">
		<result property="paymt_seq" column="PAYMT_SEQ" />
		<result property="paymt_status" column="PAYMT_STATUS" />
		<result property="price" column="PRICE" />
		<result property="paymt_date" column="MSHIP_DATE" />
		<result property="mship_grade" column="MSHIP_GRADE" />
	</resultMap>	
	 
	<!-- INSERT -->
	<insert id="insertPayment" parameterType="Payment">
		INSERT INTO PAYMENT VALUES(#{paymt_seq}, #{paymt_status}, #{paymt_method}, #{price}, #{paymt_date}, #{mship_grade})
	</insert>

</mapper>

